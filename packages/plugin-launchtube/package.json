{
    "name": "@elizaos-plugins/plugin-launchtube",
    "version": "0.1.0",
    "type": "module",
    "main": "dist/index.js",
    "module": "dist/index.js",
    "types": "dist/index.d.ts",
    "exports": {
        "./package.json": "./package.json",
        ".": {
            "import": {
                "@elizaos/source": "./src/index.ts",
                "types": "./dist/index.d.ts",
                "default": "./dist/index.js"
            }
        }
    },
    "files": [
        "dist"
    ],
    "dependencies": {
        "@elizaos/core": "workspace:^",
        "@stellar/stellar-sdk": "^12.3.0",
        "cross-fetch": "^4.0.0",
        "zod": "^3.23.8",
        "tsup": "8.3.5"
    },
    "devDependencies": {
        "@biomejs/biome": "1.5.3",
        "@types/node": "^20.0.0"
    },
    "scripts": {
        "build": "tsup --format esm --dts",
        "dev": "tsup --format esm --dts --watch",
        "lint": "biome check src/",
        "lint:fix": "biome check --apply src/",
        "format": "biome format src/",
        "format:fix": "biome format --write src/"
    },
    "agentConfig": {
        "pluginType": "elizaos:client:1.0.0",
        "pluginParameters": {
            "LAUNCHTUBE_API_KEY": {
                "type": "string",
                "description": "Launchtube JWT token for authentication"
            },
            "LAUNCHTUBE_BASE_URL": {
                "type": "string",
                "description": "Launchtube base URL (defaults to testnet)",
                "default": "https://testnet.launchtube.xyz"
            },
            "STELLAR_NETWORK": {
                "type": "string",
                "description": "Stellar network (testnet or mainnet)",
                "default": "testnet"
            }
        }
    }
}
